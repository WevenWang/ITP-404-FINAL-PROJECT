
<div  class="starred-section">

  <ul id="starredSection">
    <h3>All Celebrities</h3>


    {{#each model as |celebrity|}}
      {{#if celebrity.starred}}
        <li data-test="with-star">

          <SingleEmail @celebrity = {{celebrity}} />

        </li>

        <br>

      {{/if}}






    {{/each}}
  </ul>

</div>

<div  class="unstarred-section">

  <ul class="mainSection">


    {{#each model as |celebrity|}}


      {{#unless celebrity.starred}}

        <li data-test="without-star">

          <SingleEmail @celebrity = {{celebrity}} />

        </li>

        <br>


      {{/unless}}




    {{/each}}
  </ul>

</div>







{{outlet}}
