
<div  class="starred-section">

  <ul id="starredSection">
    <h3>Starred Email</h3>


    {{#each model as |email|}}
      {{#if email.starred}}
        <li data-test="with-star">

          <SingleEmail @email = {{email}} />

        </li>

        <br>

      {{/if}}






    {{/each}}
  </ul>

</div>

<div  class="unstarred-section">

  <ul class="mainSection">


    {{#each model as |email|}}


      {{#unless email.starred}}

        <li data-test="without-star">

          <SingleEmail @email = {{email}} />

        </li>

        <br>


      {{/unless}}




    {{/each}}
  </ul>

</div>







{{outlet}}
